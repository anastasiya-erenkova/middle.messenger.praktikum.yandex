    "jsdom": "18.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "9.1.3",
    "prettier": "2.4.1",
    "pug": "3.0.2",
    "pug-html-loader": "1.1.5",
    "pug-loader": "2.4.0",
