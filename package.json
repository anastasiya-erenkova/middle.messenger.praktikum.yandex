{
  "name": "middle.messenger.praktikum.yandex",
  "version": "1.0.0",
  "description": "Module 1. Messenger",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "start": "npm run build && npm run start:express",
    "start:express": "node ./src/app.js",
    "dev": "./node_modules/.bin/webpack",
    "build": "npm run lint && npm run build:parcel",
    "build:parcel": "parcel build src/index.html --no-cache --no-scope-hoist --no-optimize",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint src/**/*.{js,ts}",
    "stylelint": "stylelint **/*.scss",
    "prettier": "prettier --w src/**/*.{scss,ts}",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' ./node_modules/.bin/mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anastasiya-erenkova/middle.messenger.praktikum.yandex.git"
  },
