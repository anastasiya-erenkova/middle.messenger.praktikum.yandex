{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es2015",
    "isolatedModules": true,
    "moduleResolution": "node",
    "module": "es2020"
  },
}